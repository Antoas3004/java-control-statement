public class Basic_loop_3 {

	public static void main(String[] args) {
		// print n natural numbers
		// starting to 1 is called natural numbers
		int n = 5;
		for (int i = 1; i <= n; i++) {
			System.out.println(i);
		}
		// sum of n natural numbers
		int sum = 0;
		for (int i = 1; i <= n; ++i) {
			sum = sum + i;
		}
		System.out.println("sum of n natural numbers = " + sum);

		// factorial of an number
		int product = 1;
		for (int i = 1; i <= n; ++i) {
			product = product * i;
		}
		System.out.println("factorial of an number 5 = "+product);
	}
}
